# Trae Agent Configuration Template
# Copy this file to trae_config.yaml and replace placeholders with actual values

agents:
    trae_agent:
        enable_lakeview: false
        model: trae_agent_model
        max_steps: 40
        tools:
            - bash
            - str_replace_based_edit_tool
            - sequentialthinking
            - task_done
allow_mcp_servers:
    - playwright
mcp_servers:
    playwright:
        command: npx
        args:
            - "@playwright/mcp@0.0.27"
lakeview:
    model: lakeview_model

model_providers:
    openrouter:
        # Replace with your actual OpenRouter API key
        api_key: your_openrouter_api_key_here
        provider: openrouter
        base_url: https://openrouter.ai/api/v1

models:
    trae_agent_model:
        model_provider: openrouter
        model: deepseek/deepseek-chat-v3-0324:free
        max_tokens: 32768
        temperature: 0.2
        top_p: 0.95
        top_k: 0
        max_retries: 5
        parallel_tool_calls: true
    lakeview_model:
        model_provider: openrouter
        model: deepseek/deepseek-chat-v3-0324:free
        max_tokens: 32768
        temperature: 0.2
        top_p: 0.95
        top_k: 0
        max_retries: 5
        parallel_tool_calls: true
